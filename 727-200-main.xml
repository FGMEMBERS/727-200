            </autopilot>
            <autopilot n="1">
                <path>Aircraft/727-200/Systems/flight-director-v.xml</path>
            </autopilot>
            <autopilot n="2">
                <path>Aircraft/727-200/Systems/hydraulics.xml</path>
            </autopilot>
            <autopilot n="3">
                <path>Aircraft/727-200/Systems/libraries.xml</path>
            </autopilot>
		</systems>
		
		<!-- Multiplayer properties -->
		<multiplay>
			<generic>
				<float n="0" alias="/controls/flight/elevator-left" />
				<float n="1" alias="/controls/flight/elevator-right" />
				<float n="2" alias="/controls/flight/aileron-left" />
				<float n="3" alias="/controls/flight/aileron-right" />
				<float n="4" alias="/controls/flight/rudder-upper" />
				<float n="5" alias="/controls/flight/rudder-lower" />
				<float n="6" alias="/fdm/jsbsim/fcs/slat-pos-norm" />
			</generic>
		</multiplay>
	</sim>
	
