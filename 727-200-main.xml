<?xml version="1.0"?>

<PropertyList>

	<sim>
		<author>Joshua Davidson (it0uchpods)</author>
		
		<status>Pre V1.0</status>
		
		<rating>
			<FDM type="int">3</FDM>
			<systems type="int">2</systems>
			<cockpit type="int">3</cockpit>
			<model type="int">3</model>
		</rating>
		
		<sound>
			<path>Aircraft/727-200/Sounds/Sounds.xml</path>
			<switch type="bool">0</switch>
			<stall-horn type="bool">0</stall-horn>
		</sound>
		
		<startup>
			<splash-texture>Aircraft/727-200/Splash/SplashOld.png</splash-texture>
		</startup>
		
		<previews>
			<preview>
				<type>exterior</type>
				<splash type="bool">true</splash>
				<path>Splash/splash1.png</path>
			</preview>
		</previews>
		
		<tags>
			<tag>transport</tag>
			<tag>boeing</tag>
			<tag>1960s</tag>
			<tag>1970s</tag>
			<tag>1980s</tag>
			<tag>1990s</tag>
			<tag>ifr</tag>
			<tag>retractable-gear</tag>
			<tag>glass-cockpit</tag>
			<tag>pressurised</tag>
			<tag>jet</tag>
			<tag>3-engine</tag>
		</tags>

		<menubar>
			<default>
				<menu n="10">
					<label>727-2XX</label>
					<enabled type="bool">true</enabled>
					<item>
						<label>Magic Startup</label>
						<binding>
							<command>nasal</command>
							<script>
							libraries.doMagicStartup();
							</script>
						</binding>
					</item>
					<item>
						<label>Pushback</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>pushback</dialog-name>
						</binding>
					</item>
					<item>
						<label>Lights</label>
						<binding>
							<command>nasal</command>
							<script>
							var lights = gui.Dialog.new("sim/gui/dialogs/lights/dialog", "Aircraft/727-200/Systems/lights-dlg.xml");
							lights.open();
							</script>
						</binding>
					</item>
					<item>
						<label>Select Livery</label>
						<binding>
							<command>nasal</command>
							<script>aircraft.livery.dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>FCTL</label>
						<binding>
							<command>nasal</command>
							<script>
							var fctl = gui.Dialog.new("sim/gui/dialogs/fctl/dialog", "Aircraft/727-200/Systems/fctl-dlg.xml");
							fctl.open();
							</script>
						</binding>
					</item>
				</menu>
			</default>
		</menubar>
		
		<tags>
			<tag>transport</tag>
			<tag>boeing</tag>
			<tag>1960s</tag>
			<tag>1970s</tag>
			<tag>1980s</tag>
			<tag>1990s</tag>
			<tag>ifr</tag>
			<tag>retractable-gear</tag>
			<tag>pressurised</tag>
			<tag>jet</tag>
			<tag>3-engine</tag>
		</tags>
		
		<flaps>
			<setting>0.000</setting> <!-- Flaps 0 -->
			<setting>0.050</setting> <!-- Flaps 2 -->
			<setting>0.125</setting> <!-- Flaps 5 -->
			<setting>0.375</setting> <!-- Flaps 15 -->
			<setting>0.625</setting> <!-- Flaps 25 -->
			<setting>0.750</setting> <!-- Flaps 30 -->
			<setting>1.000</setting> <!-- Flaps 40 -->
		</flaps>
		
		<weight n="0">
			<name>156 PAX</name>
			<weight-lb>20080</weight-lb>
			<max-lb>20080</max-lb>
		</weight>

		<weight n="1">
			<name>pilots</name>
			<weight-lb>340</weight-lb>
			<max-lb>340</max-lb>
		</weight>
		
		<instrumentation>
			<path>Aircraft/727-200/Systems/instrumentation.xml</path>
		</instrumentation>
		
		<systems n="0">
			<!--electrical>
				<path></path>
			</electrical-->
            <autopilot n="0">
