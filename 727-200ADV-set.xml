<?xml version="1.0"?>

<PropertyList include="727-200-main.xml">

	<sim>
		<description>Boeing 727-200 Advanced</description>
		<flight-model>jsb</flight-model>
		<aero>727-200ADV</aero>

		<model>
			<path>Aircraft/727-200/Models/727-200ADV.xml</path>
			<livery>
				<name type="string">Boeing 727</name>
				<texture>Liveries-ADV/BOE.png</texture>
			</livery>

			<pushback>
				<kp type="double">1600</kp>
				<ki type="double">0</ki>
				<kd type="double">0</kd>
				<position-norm type="double">0</position-norm>
				<enabled type="bool">false</enabled>
			</pushback>
		</model>

		<view>
			<internal archive="y">true</internal>
			<config>
				<x-offset-m archive="y">-0.44</x-offset-m>
				<y-offset-m archive="y">1.05</y-offset-m>
				<z-offset-m archive="y">-20.44</z-offset-m>
				<pitch-offset-deg archive="y">-12.0</pitch-offset-deg>
				<default-field-of-view-deg type="double">60.8</default-field-of-view-deg>
			</config>
		</view>
		<view n="100">
			<name>Copilot View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.44</x-offset-m>
				<y-offset-m archive="y">1.05</y-offset-m>
				<z-offset-m archive="y">-20.44</z-offset-m>
				<pitch-offset-deg archive="y">-12.0</pitch-offset-deg>
				<default-field-of-view-deg type="double">60.8</default-field-of-view-deg>
			</config>
		</view>
		<view n="101">
			<name>Left Front View</name>
			<type>lookfrom</type>
			<internal archive="y">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">-2.03</x-offset-m>
				<y-offset-m archive="y">0.97</y-offset-m>
				<z-offset-m archive="y">-5.56</z-offset-m>
				<heading-offset-deg>115</heading-offset-deg>
				<default-field-of-view-deg type="double">90.0</default-field-of-view-deg>
			</config>
		</view>
		<view n="102">
			<name>Right Mid View</name>
			<type>lookfrom</type>
			<internal archive="y">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">2.03</x-offset-m>
				<y-offset-m archive="y">0.97</y-offset-m>
				<z-offset-m archive="y">-0.32</z-offset-m>
				<heading-offset-deg>278</heading-offset-deg>
				<default-field-of-view-deg type="double">90.0</default-field-of-view-deg>
			</config>
		</view>
		<view n="103">
			<name>Left Aft View</name>
			<type>lookfrom</type>
			<internal archive="y">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">-2.03</x-offset-m>
				<y-offset-m archive="y">0.97</y-offset-m>
				<z-offset-m archive="y">5.95</z-offset-m>
				<heading-offset-deg>88</heading-offset-deg>
				<default-field-of-view-deg type="double">90.0</default-field-of-view-deg>
			</config>
		</view>

		<chase-distance-m>-100</chase-distance-m>

	</sim>
	
	<nasal n="0">
		<specific>
			<file>Aircraft/727-200/Nasal/722adv-livery.nas</file>
		</specific>
	</nasal>

</PropertyList>
