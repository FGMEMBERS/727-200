<PropertyList include="727-200-main.xml">
	
	<sim>
		<description>Boeing 727-200 Advanced (JT8D-17R)</description>
		<flight-model>jsb</flight-model>
		<aero>727-200ADV</aero>
		
		<model>
			<path>Aircraft/727-200/Models/727-200ADV.xml</path>
			<livery>
				<file>Boeing 727</file>
			</livery>
		</model>
		
		<chase-distance-m>-100</chase-distance-m>
		
		<systems n="0">
            <autopilot n="4">
                <path>Aircraft/727-200/Systems/JT8D-9-15-17.xml</path>
            </autopilot>
		</systems>
	
	</sim>
	
	<controls n="0">
		<engines n="0">
			<engine n="0">
				<epr-lim type="double">2.11</epr-lim>
			</engine>
			<engine n="1">
				<epr-lim type="double">2.11</epr-lim>
			</engine>
			<engine n="2">
				<epr-lim type="double">2.11</epr-lim>
			</engine>
		</engines>
	</controls>
	
	<nasal n="0">
		<specific>
			<file>Aircraft/727-200/Nasal/722adv-livery.nas</file>
			<file>Aircraft/727-200/Nasal/engines-3rev.nas</file>
		</specific>
	</nasal>

</PropertyList>
