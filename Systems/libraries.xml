<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	
    <filter>
        <name>EGT 1</name>
        <type>gain</type>
        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input>
            <expression>
                <table>
                    <property>/engines/engine[0]/n1</property> 
                    <entry><ind>0</ind><dep>0</dep></entry>
                    <entry><ind>6</ind><dep>0</dep></entry>
                    <entry><ind>33.4</ind><dep>324</dep></entry>
                    <entry><ind>110</ind><dep>612</dep></entry>
                </table>
            </expression>
        </input>
		<output>/engines/engine[0]/egt-actual</output>
    </filter>
	
    <filter>
        <name>EGT 2</name>
        <type>gain</type>
        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input>
            <expression>
                <table>
                    <property>/engines/engine[1]/n1</property> 
                    <entry><ind>0</ind><dep>0</dep></entry>
                    <entry><ind>6</ind><dep>0</dep></entry>
                    <entry><ind>33.4</ind><dep>324</dep></entry>
                    <entry><ind>110</ind><dep>612</dep></entry>
                </table>
            </expression>
        </input>
		<output>/engines/engine[1]/egt-actual</output>
    </filter>
	
    <filter>
        <name>EGT 3</name>
        <type>gain</type>
        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input>
            <expression>
                <table>
                    <property>/engines/engine[2]/n1</property> 
                    <entry><ind>0</ind><dep>0</dep></entry>
                    <entry><ind>6</ind><dep>0</dep></entry>
                    <entry><ind>33.4</ind><dep>324</dep></entry>
                    <entry><ind>110</ind><dep>612</dep></entry>
                </table>
            </expression>
        </input>
		<output>/engines/engine[2]/egt-actual</output>
    </filter>
	
	<filter>
		<name>Throttle 1</name>
		<type>gain</type>
		<gain>1</gain>
		<input>
			<condition>
				<equals>
					<property>/controls/engines/engine[0]/reverser</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/controls/engines/engine[0]/throttle</property>
		</input>
		<input>0</input>
		<output>/controls/engines/engine[0]/throttle-pos</output>
	</filter>
	
	<filter>
		<name>Throttle 1</name>
		<type>noise-spike</type>
		<input>
			<condition>
				<equals>
					<property>/controls/engines/engine[0]/reverser</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/controls/engines/engine[0]/throttle-pos</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/controls/engines/engine[0]/reverser</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/controls/engines/engine[0]/throttle-rev</property>
			<scale>1.3</scale>
		</input>
		<output>/controls/engines/engine[0]/throttle-fdm</output>
		<max-rate-of-change>0.65</max-rate-of-change>
	</filter>
	
	<filter>
		<name>Throttle 2</name>
		<type>gain</type>
		<gain>1</gain>
		<input>
			<condition>
				<equals>
					<property>/controls/engines/engine[1]/reverser</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/controls/engines/engine[1]/throttle</property>
		</input>
		<input>0</input>
		<output>/controls/engines/engine[1]/throttle-pos</output>
	</filter>
	
	<filter>
		<name>Throttle 2</name>
		<type>noise-spike</type>
		<input>
			<condition>
				<equals>
					<property>/controls/engines/engine[1]/reverser</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/controls/engines/engine[1]/throttle-pos</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/controls/engines/engine[1]/reverser</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/controls/engines/engine[1]/throttle-rev</property>
			<scale>1.3</scale>
		</input>
		<output>/controls/engines/engine[1]/throttle-fdm</output>
		<max-rate-of-change>0.65</max-rate-of-change>
	</filter>
	
	<filter>
		<name>Throttle 3</name>
		<type>gain</type>
		<gain>1</gain>
		<input>
			<condition>
				<equals>
					<property>/controls/engines/engine[2]/reverser</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/controls/engines/engine[2]/throttle</property>
		</input>
		<input>0</input>
		<output>/controls/engines/engine[2]/throttle-pos</output>
	</filter>
	
	<filter>
		<name>Throttle 3</name>
		<type>noise-spike</type>
		<input>
			<condition>
				<equals>
					<property>/controls/engines/engine[2]/reverser</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/controls/engines/engine[2]/throttle-pos</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/controls/engines/engine[2]/reverser</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/controls/engines/engine[2]/throttle-rev</property>
			<scale>1.3</scale>
		</input>
		<output>/controls/engines/engine[2]/throttle-fdm</output>
		<max-rate-of-change>0.65</max-rate-of-change>
	</filter>
	
	<filter>
		<type>gain</type>
		<gain>1</gain>
		<input>
			<expression>
				<dif>
					<property>/instrumentation/altimeter/indicated-altitude-ft</property>
					<property>/it-autoflight/internal/alt</property>
				</dif>
			</expression>
		</input>
		<output>
			<property>sim/sounde/altalert</property>
		</output>
	</filter>

	<filter>
        <name>Window Heat Factor</name>
        <type>gain</type>
        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input> 
            <expression>
                <table>
                    <property>velocities/TAS</property> 
                    <entry><ind>0</ind><dep>0.0016</dep></entry>
					<entry><ind>200</ind><dep>0.0014</dep></entry>
                    <entry><ind>400</ind><dep>0.0008</dep></entry>
                </table>
            </expression>
        </input>
		<output>/systems/heat/heatingfactor</output>
    </filter>
	
	<filter>
        <name>TAS</name>
        <type>gain</type>
        <gain>1</gain>
        <input> 
			 <expression>
                <product>
                    <property>velocities/uBody-fps</property> 
                    <value>0.592484</value>
                </product>
            </expression>
		</input>
		<output>/velocities/TAS</output>
    </filter>
	
</PropertyList>
